{
  "private": true,
  "name": "cookie-though-preact",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit && parcel build src/index.html --no-source-maps --global CookieThough --out-dir public",
    "dev": "parcel src/index.html --global CookieThough",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "markdown:fix": "markdownlint '**/*.md' --ignore node_modules --fix",
    "prettier:fix": "npm run prettier -- --write",
    "prettier": "prettier \"src/**/*.{js,ts,tsx}\"",
    "postinstall": "./scripts/postinstall-local.sh",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN",
    "test": "jest",
    "test:coverage": "jest --coverage --reporters=jest-junit"
  },
  "eslintIgnore": [
    "dist/*"
  ],
  "dependencies": {
    "preact": "10.5.9",
    "react-collapse": "5.1.0"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  },
  "browserslist": [
    "> 0.25%",
    "last 2 versions",
    "IE >= 9"
  ],
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-react-jsx": "7.12.12",
    "@types/enzyme": "3.10.8",
    "@types/jest": "26.0.20",
    "@types/react-collapse": "5.0.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "autoprefixer": "9.8.6",
    "css-loader": "3.6.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-preact-pure": "3.0.0",
    "enzyme-to-json": "3.6.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "3.0.0",
    "firebase-tools": "9.1.0",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "jest-preset-preact": "4.0.2",
    "markdownlint-cli": "0.26.0",
    "parcel-bundler": "1.12.4",
    "postcss-modules": "3.2.2",
    "prettier": "2.2.1",
    "sirv-cli": "1.0.10",
    "typescript": "3.9.7"
  }
}
